#include <stdio.h>

/**
 * main - this function will print out the alphabet
 * putchar - will display the alphabet (replace printf)
 * @char: is the string of alphabet
 * Return: will return 0
 */

int main(void)
{
char a[26] = "abcdefghijklmnopqrstuvwxyz";
int i;
i = 0;
while
(i < 26)
{
i++;
}
{
if
(i == 5)
{
i++;
}
else if
(i == 17)
{
i++;
}
else
{
putchar(a[i]);
i++;
}
putchar('\n');

return (0);
}
